<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="acquisition_agent_report_invoice_document" inherit_id="account.report_invoice_document">
        <xpath expr="//div[@id='informations']" position="after">
            <div class="col-auto col-3 mw-100 mb-2" t-if="o.acquisition_agent" name="reference">
                <strong>Acquisition Agent:</strong>
                <p class="m-0" t-field="o.acquisition_agent"/>
            </div>
        </xpath>
    </template>
</odoo>
